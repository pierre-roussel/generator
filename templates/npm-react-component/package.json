{
  "version": "0.0.1",
  "author": "<%= domain %>",
  "bugs": {
    "url": "https://github.com/thredup/shamrock/issues"
  },
  "dependencies": {
    "<%= author %>/styleguide": "*",
    "classnames": "*",
    "prop-types": "*",
    "react": "*",
    "react-router-dom": "*"
  },
  "description": "<%= customName %>: <%= customDescription %>",
  "devDependencies": {
    "@types/classnames": "*",
    "@types/enzyme": "*",
    "@types/enzyme-adapter-react-16": "*",
    "@types/react": "*",
    "@types/react-router-dom": "*",
    "enzyme": "*",
    "enzyme-adapter-react-16": "*",
    "jest": "*",
    "rollup": "*"
  },
  "externals": [
    "<%= author %>/styleguide",
    "@types/classnames",
    "@types/enzyme",
    "@types/enzyme-adapter-react-16",
    "@types/react",
    "@types/react-router-dom",
    "classnames",
    "enzyme",
    "enzyme-adapter-react-16",
    "jest",
    "prop-types",
    "react",
    "rollup"
  ],
  "keywords": [
    "thredup"
  ],
  "license": "UNLICENSED",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "name": "<%= author %>/<%= name %>",
  "private": false,
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thredup/shamrock.git"
  },
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "NODE_ENV=development rollup  --config-ext tsx -c ../../../build/bundler/rollup.config.babel.js -w",
    "build:prod": "NODE_ENV=production rollup  --config-ext tsx -c ../../../build/bundler/rollup.config.babel.js",
    "test": "jest -c ../../../jest.config.js --testPathPattern='packages/private/<%= name %>/'",
    "test:watch": "jest -c ../../../jest.config.js --watchAll --watch --testPathPattern='packages/private/<%= name %>/'"
  },
  "gitHead": "19f8a100420cf497d91eeb39e28465f3a59ff332"
}
